<query id="getFilesCount" action="select">
    <tables>
        <table name="files" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="upload_target_srl" var="upload_target_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
